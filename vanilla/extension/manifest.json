{
    "manifest_version": 3,
    "name": "Kansodo",
    "description": "Infinite to-do list in browser",
    "version": "1.0",
    "permissions": [
        "tabs",
        "activeTab"
    ],
    "action": {
      "default_popup": "index.html",
      "default_icon": "hello.png"
    },
    "chrome_url_overrides" : {
        "newtab": "index.html"
    }
  }